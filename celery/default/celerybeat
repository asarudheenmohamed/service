# enable the correct one for stagin/production
export DJANGO_SETTINGS_MODULE=config.settings.staging
export DJANGO_SETTINGS_MODULE=config.settings.prod

# Absolute or relative path to the 'celery' command:
#CELERY_BIN="/usr/local/bin/celery"
CELERY_BIN="/home/django/Env/tendercuts/bin/celery"

# App instance to use
# comment out this line if you don't use an app
CELERY_APP="config"
# or fully qualified:
#CELERY_APP="proj.tasks:app"

# Where to chdir at start.
CELERYBEAT_CHDIR="/home/django/api/tendercuts"

# Extra arguments to celerybeat
CELERYBEAT_OPTS="--schedule=/var/run/celery/celerybeat-schedule"

CELERYBEAT_USER="django"
CELERYBEAT_GROUP="django"
